{
  "name": "jmFamily",
  "version": "0.3.1",
  "description": "a family database",
  "repository": {
    "type": "git",
    "url": "https://github.com/jmcpeak/family.git"
  },
  "main": "app/index.js",
  "author": "Jason McPeak",
  "license": "ISC",
  "scripts": {
    "prestart": "npm install",
    "start": "node node_modules/webpack-dev-server/bin/webpack-dev-server.js --color --content-base app/ --hot --port 9000",
    "build": "rm -rf dist && NODE_ENV=production node node_modules/.bin/webpack && cp app/* dist || mkdir dist/assets && cp app/assets/*.png dist/assets",
    "distserve": "npm install && npm run build && node server/distserver",
    "pretest": "npm install",
    "test": "karma start",
    "coverage": "karma start karma.conf.js --coverage",
    "test-single-run": "karma start karma.conf.js  --single-run",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor e2e-tests/protractor.conf.js",
    "update-index-async": "node -e \"require('shelljs/global'); sed('-i', /\\/\\/@@NG_LOADER_START@@[\\s\\S]*\\/\\/@@NG_LOADER_END@@/, '//@@NG_LOADER_START@@\\n' + sed(/sourceMappingURL=angular-loader.min.js.map/,'sourceMappingURL=bower_components/angular-loader/angular-loader.min.js.map','app/bower_components/angular-loader/angular-loader.min.js') + '\\n//@@NG_LOADER_END@@', 'app/index-async.html');\"",
    "p": "node node_modules/.bin/webpack"
  },
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.2",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "css-loader": "*",
    "del": "^1.2.0",
    "expose-loader": "^0.7.0",
    "express": "^4.13.3",
    "file-loader": "^0.8.4",
    "glob": "^5.0.15",
    "html-loader": "^0.3.0",
    "imports-loader": "^0.6.5",
    "isparta-loader": "^0.2.0",
    "istanbul-instrumenter-loader": "^0.1.3",
    "jasmine-core": "^2.3.4",
    "jquery": "^2.1.4",
    "json-loader": "^0.5.3",
    "karma": "latest",
    "karma-chrome-launcher": "*",
    "karma-coverage": "latest",
    "karma-htmlfile-reporter": "*",
    "karma-jasmine": "*",
    "karma-ng-html2js-preprocessor": "^0.1.2",
    "karma-sourcemap-loader": "*",
    "karma-spec-reporter": "*",
    "karma-webpack": "^1.7.0",
    "lazypipe": "^1.0.1",
    "less": "^2.5.3",
    "less-css-stream": "^1.0.0",
    "less-loader": "^2.2.0",
    "raw-loader": "^0.5.1",
    "style-loader": "*",
    "url-loader": "^0.5.6",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "files": [
    "app/**/*.tpl.html",
    "app/**/*.less",
    "app/favicon.ico",
    "package.json",
    "README.md"
  ],
  "dependencies": {
    "angular": "^1.5.8",
    "angular-animate": "^1.5.8",
    "angular-aria": "^1.5.8",
    "angular-clipboard": "^1.4.2",
    "angular-material": "angular/bower-material",
    "angular-messages": "^1.5.8",
    "angular-mocks": "^1.5.8",
    "angular-route": "^1.5.8",
    "angular-ui-router": "^0.3.1",
    "aws-sdk": "^2.3.17",
    "html-webpack-plugin": "^2.22.0",
    "moment": "^2.13.0",
    "ngstorage": "^0.3.10"
  }
